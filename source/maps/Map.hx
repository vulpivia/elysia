package maps;

/**
    The base class of a map.
**/
class Map
{
    /**
        Path to the bottom part of the map.
    **/
    public var bottomCSV:String;
    /**
        Path to the top part of the map.
    **/
    public var topCSV:String;
    /**
        Path to the collision map.
    **/
    public var collisionCSV:String;
}